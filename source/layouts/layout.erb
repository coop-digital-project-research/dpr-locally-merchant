<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">

    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "Locally Merchant" %></title>

    <%= stylesheet_link_tag :site %>
    <%= javascript_include_tag :grid %>
    <%= javascript_include_tag :version %>
    <%= javascript_include_tag :detector %>
    <%= javascript_include_tag :formatinf %>
    <%= javascript_include_tag :errorlevel %>
    <%= javascript_include_tag :bitmat %>
    <%= javascript_include_tag :datablock %>
    <%= javascript_include_tag :bmparser %>
    <%= javascript_include_tag :datamask %>
    <%= javascript_include_tag :rsdecoder %>
    <%= javascript_include_tag :gf256poly %>
    <%= javascript_include_tag :gf256 %>
    <%= javascript_include_tag :decoder %>
    <%= javascript_include_tag :qrcode %>
    <%= javascript_include_tag :findpat %>
    <%= javascript_include_tag :alignpat %>
    <%= javascript_include_tag :databr %>
    <%= javascript_include_tag :all %>
  </head>

  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>
